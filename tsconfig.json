{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [ "es2015", "dom" ],
    "types": [
      "node",
      "mocha"
    ]
  },
  "exclude": [
    "node_modules/*",
    "src/client/app/prod.ts",
    "src/client/dist/*"
  ]
}
